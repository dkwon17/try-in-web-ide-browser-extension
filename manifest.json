{
  "manifest_version": 3,
  "name": "Try in Dev Spaces",
  "browser_specific_settings": {
    "gecko": {
      "id": "{e5b67b9e-a1bd-11ed-a8fc-0242ac120002}"
    }
  },
  "optional_host_permissions": ["https://*/*", "http://*/*"],
  "host_permissions": ["https://github.com/*"],
  "background": {
    "service_worker": "backgroundScript.bundle.js"
  },
  "permissions": [
    "storage", "scripting"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
