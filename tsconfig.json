{
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "esnext",
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "outDir": "./dist",
    "rootDir": ".",
    "noEmit": false,
    "target": "es2015",
    "esModuleInterop": true,
    "skipLibCheck": true,
  },
  "include": [
    "src"
  ]
}
